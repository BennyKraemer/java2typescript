{
    "version": "1.5.0-beta",
    "compilerOptions": {
        "target": "es5",
        "isolatedModules": false,
        "jsx": "react",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "noImplicitAny": true,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "outDir": "../../../../target/test-classes/global-test"
    },
    "filesGlob": [
        "./src/**/*.ts",
        "../../../../target/classes/global/**/*.d.ts"
    ],
    "files": [
        "./src/test.ts",
        "../../../../target/classes/global/java.d.ts",
        "../../../../target/classes/global/junit.d.ts"
    ]
}
